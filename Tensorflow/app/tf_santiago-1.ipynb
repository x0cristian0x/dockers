    "class MPNeuron:\n",
    "    def __init__(self):\n",
    "        self.threshold = None\n",
    "\n",
    "    def model(self,x):\n",
    "        z = sum(x)\n",
    "        return (sum(x)>= self.threshold)\n",
    "    \n",
    "    def predict(self, X):\n",
    "        Y = []\n",
    "        for x in X:\n",
    "            result = self.model(x)\n",
    "            Y.append(result)\n",
    "        return np.array(Y)      "
   ]
